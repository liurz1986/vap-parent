    <update id="cancelFirstPage">
      update visual_dashboard
      set first_flag = 0
      where 1=1;
    </update>

  <select id="findMaxTop" resultType="Integer">
    SELECT (IFNULL(MAX(CAST(top AS UNSIGNED)),0) + 1) first_flag FROM visual_dashboard
  </select>
