<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vrv.vap.line.mapper.BaseLineSourceMapper">
    <insert id="saveBatch4List" parameterType="java.util.List">
        INSERT INTO base_line_source
        (
        id,
        `title`,
        `name`,
        `time_field`,
        `time_format`,
        `description`
        )
        VALUES
        <foreach collection="list" item="item" separator=",">
            (
            #{item.id},
            #{item.title},
            #{item.name},
            #{item.timeField},
            #{item.timeFormat},
            #{item.description}
            )
        </foreach>
    </insert>
</mapper>
