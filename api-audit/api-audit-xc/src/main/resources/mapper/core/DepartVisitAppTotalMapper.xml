<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vrv.vap.xc.mapper.core.DepartVisitAppTotalMapper">


    <insert id="replaceInto" parameterType="java.util.List">
        REPLACE INTO rpt_depart_visit_app_total
        (depart_no,depart_name,app_count,visit_num,`time`,version)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (
            #{item.departNo},
            #{item.departName},
            #{item.appCount},
            #{item.visitNum},
            #{item.time},
            #{item.version}
            )
        </foreach>
    </insert>

</mapper>
